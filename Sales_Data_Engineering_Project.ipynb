    Step 2: Load Dataset
  
    "file_path = 'sales_data_sample.csv'\n",
    
    "df = pd.read_csv(file_path, encoding='latin1')\n",
    "print(f\" Data loaded successfully: {df.shape} rows\")\n",
    "df.head()"

     Step 3: Data Transformation & KPI Creation
 
    "df['COST'] = df['PRICEEACH'] * df['QUANTITYORDERED'] * 0.7\n",
    "df['PROFIT_MARGIN'] = round(((df['SALES'] - df['COST']) / df['SALES']) * 100, 2)\n",
    
    
    "df.to_csv('sales_data_cleaned.csv', index=False)\n",
    "print('âœ… Cleaned dataset with Profit Margin KPI saved!')\n",
    "df[['ORDERNUMBER','SALES','COST','PROFIT_MARGIN']].head()"

     Step 4: Simulate Data Warehouse Using SQLite

    "conn = sqlite3.connect('sales_dw.db')\n",
    "df.to_sql('sales_data', conn, if_exists='replace', index=False)\n",
    
    "query = '''\n",
    "SELECT COUNTRY, YEAR_ID, ROUND(AVG(PROFIT_MARGIN), 2) AS AVG_MARGIN, SUM(SALES) AS TOTAL_SALES\n",
    "FROM sales_data\n",
    "GROUP BY COUNTRY, YEAR_ID\n",
    "ORDER BY TOTAL_SALES DESC;\n",
    "'''\n",
   
    "summary = pd.read_sql(query, conn)\n",
    "summary.head()"
     
     Step 5: Data Visualization
  
  
    "plt.figure(figsize=(10,5))\n",
    "sns.barplot(data=df.groupby('COUNTRY')['PROFIT_MARGIN'].mean().reset_index(),\n",
    "            x='COUNTRY', y='PROFIT_MARGIN', palette='Blues_d')\n",
    "plt.title('Average Profit Margin by Country')\n",
    "plt.xticks(rotation=45)\n",
    "plt.show()"
   
